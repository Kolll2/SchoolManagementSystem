<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="rootPane" prefHeight="220.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.addperson.FXMLDocumentController">
   <children>
      <VBox prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="0.0">
         <children>
            <JFXTextField fx:id="firstName" layoutX="10.0" layoutY="10.0" promptText="FirstName" stylesheets="@FXMLDocument.css">
               <VBox.margin>
                  <Insets bottom="5.0" top="15.0" />
               </VBox.margin></JFXTextField>
            <JFXTextField fx:id="secondName" promptText="SecondName">
               <VBox.margin>
                  <Insets bottom="5.0" top="10.0" />
               </VBox.margin></JFXTextField>
            <JFXTextField fx:id="middleName" layoutX="10.0" layoutY="10.0" promptText="MiddleName">
               <VBox.margin>
                  <Insets bottom="5.0" top="10.0" />
               </VBox.margin></JFXTextField>
            <JFXTextField fx:id="ID" layoutX="10.0" layoutY="35.0" promptText="System ID">
               <VBox.margin>
                  <Insets bottom="5.0" top="10.0" />
               </VBox.margin></JFXTextField>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <JFXButton fx:id="save" onAction="#save" prefHeight="40.0" prefWidth="200.0" stylesheets="@FXMLDocument.css" text="Save" />
                  <JFXButton fx:id="cancel" layoutX="10.0" layoutY="10.0" onAction="#cancel" prefHeight="40.0" prefWidth="125.0" styleClass="jfx-button-cancel" stylesheets="@FXMLDocument.css" text="Cancel">
                     <HBox.margin>
                        <Insets left="50.0" />
                     </HBox.margin></JFXButton>
               </children>
               <VBox.margin>
                  <Insets bottom="10.0" top="10.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
